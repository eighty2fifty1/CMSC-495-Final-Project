+    # see: rfc7043.txt
+
+    __slots__ = ["eui"]
+    # define these in subclasses
+    # byte_len = 6  # 0123456789ab (in hex)
+    # text_len = byte_len * 3 - 1  # 01-23-45-67-89-ab
