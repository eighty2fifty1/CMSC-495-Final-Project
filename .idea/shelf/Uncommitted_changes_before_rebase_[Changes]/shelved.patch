+class Version:
+    def __init__(
+        self,
+        zone: Zone,
+        id: int,
+        nodes: Optional[MutableMapping[dns.name.Name, dns.node.Node]] = None,
+        origin: Optional[dns.name.Name] = None,
+    ):
+        self.zone = zone
+        self.id = id
+        if nodes is not None:
+            self.nodes = nodes
+        else:
+            self.nodes = zone.map_factory()
+        self.origin = origin
+
+    def _validate_name(self, name: dns.name.Name) -> dns.name.Name:
